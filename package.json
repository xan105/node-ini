{
  "name": "@xan105/ini",
  "version": "2.1.1",
  "description": "Yet another (opinionated) ini encoder/decoder for Node.js.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "types": "./types/index.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "lint": "eslint \"./lib/**/*.{js,mjs}\" \"./types/**/*.d.ts\"",
    "test": "tap \"./test/**/*.js\" --no-coverage --no-coverage-report",
    "check": "tsc --noemit --checkjs",
    "declare": "tsc --declaration --emitDeclarationOnly --outDir \"./types\""
  },
  "keywords": [
    "ini"
  ],
  "author": {
    "name": "Anthony Beaumont",
    "email": "dev.xan105@outlook.be",
    "url": "https://xan105.com/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xan105/node-ini.git"
  },
  "bugs": {
    "url": "https://github.com/xan105/node-ini/issues"
  },
  "homepage": "https://github.com/xan105/node-ini#readme",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/xan105"
    },
    {
      "type": "paypal",
      "url": "https://www.paypal.me/xan105"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/xan105"
    }
  ],
  "devDependencies": {
    "@types/node": "^18.11.12",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "tap": "^16.3.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@xan105/is": "^2.7.0"
  }
}
